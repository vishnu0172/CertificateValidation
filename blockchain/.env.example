# Rename this file to .env and fill in your values

# Your wallet private key (NEVER commit this to git!)
PRIVATE_KEY=your_metamask_private_key_here

# Alchemy/Infura API key for blockchain connection
ALCHEMY_API_KEY=your_alchemy_api_key_here

# OR use Infura instead
INFURA_API_KEY=your_infura_api_key_here

# Polygonscan API key (for contract verification)
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# Etherscan API key (for Ethereum networks)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
